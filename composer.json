{
  "name": "revolution/salvager",
  "description": "WebCrawler build with Laravel Dusk and Symfony DomCrawler",
  "keywords": [
    "laravel",
    "dusk",
    "dom-crawler",
    "chrome"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.3",
    "laravel/dusk": "^5.0",
    "symfony/dom-crawler": "^4.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "~3.7"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Salvager\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Salvager\\Providers\\SalvagerServiceProvider"
      ],
      "aliases": {
        "Salvager": "Revolution\\Salvager\\Facades\\Salvager"
      }
    }
  }
}
